<%= form_for (@contact), :class => "" do |f| %>
    
    <div class="col-md-8">
  <% if notice %>
    <p class="alert alert-success"><%= notice %></p>
<% end %>
      <div class="working-hours-title">Say Hello!</div>
      <div class="row">
        <div class="col-md-4">
          <%= f.text_field :name, autofocus: true, required: true, :placeholder => "Full Name" %>
        </div>
        <div class="col-md-4">
          <%= f.email_field :email, required: true, :placeholder => "Email Address"%>
        </div>
        <div class="col-md-4">
          <%= f.text_field :phone_number, :placeholder => "Phone Number" %>
        </div>
      </div>
      <div class="row">
        <div class="col-md-12">
          <%= f.text_area :message, required: true, :placeholder => "Your Message" %>
        </div>
      </div>
      <div class="row">
        <div class="col-md-12">
          <%= f.submit "Send Message", :class => "contact-submit" %>
        </div>
      </div>
    </div>
  </div>
</div>
<% end %>
